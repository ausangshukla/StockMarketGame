<tr id="order-<%=order.id%>">
    <td><%= link_to "Order-#{order.id}", order %></td>
    <td><%= order.side %></td>
    <td><%= link_to order.symbol, security_path(id: order.security_id) %></td>
    <td><%= order.quantity %></td>
    <td><%= order.price %></td>
    <td><%= order.price_type %></td>
    <td><%= order.fill_status %></td>
    <td><%= order.filled_qty %></td>
    <td><%= order.open_qty %></td>
    <td><%= order.status %></td>
    
</tr>